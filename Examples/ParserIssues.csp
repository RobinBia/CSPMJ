P(ch,<>) = SKIP
P(ch,x) = ch.head(x) -> P(ch,tail(x))

P1 = P(c,x1)

procs = <P(i,<1,2,3>),P(c,<>), (c.true -> SKIP)>

Procs(<>) = STOP
Procs(ps) = head(ps);Procs(tail(ps))

Procs1 = Procs(procs)

