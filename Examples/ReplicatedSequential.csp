
-- ReplicatedSequential


channel out:{0..99}


Gen(n,m) = if n<m then out!n -> Gen(n+1,m) else SKIP

PrmSeq = (<2,3,5,7,11>)

EqSkip = ; x: <> @ Gen(1,100)

PrGen = ; x:tail(PrmSeq) @ Gen(x,x+2)

MAIN = EqSkip ; PrGen

